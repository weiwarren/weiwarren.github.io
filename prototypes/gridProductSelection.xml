			<TD FIELD="vcTowGroupingDesc" SORTABLE="yes" SORTED="no" DATATYPE="text" ORDER="ascending" ALIGN="left">
				<NOBR>
					<LABEL ID="CHchTowGrouping" TERMCODE="chTowGrouping">Product Group</LABEL>
					<IMG SRC="images/imgGridBarUp.gif"/>
				</NOBR>
			</TD>
			<TD TYPE="SEP" SORTABLE="no" RESIZABLE="yes"><IMG SRC="images/imgGridBarColSep.gif"/></TD>
