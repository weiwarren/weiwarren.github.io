		<TREFHEAD>
			<COLGROUP>
				<COL WIDTH="15"/>
			</COLGROUP>
			<TR>
				<TD SORTABLE="no" ALIGN="center" ONCLICK="fnGridProductListRefresh_onClick();" STYLE="cursor:hand"><IMG SRC="images/imgGridBarRefresh.gif"/></TD>
			</TR>
		</TREFHEAD>
	</THEAD>
	<TBODY DATASRC="#xmlDataProductList" DATAFLD="Table" SELMODE="single" SELROW="" STYLE="DISPLAY:none;">
		<COLGROUP>
		<COL WIDTH="57"/>
		<COL WIDTH="122"/>
		<COL WIDTH="202"/>
		<COL WIDTH="82"/>
		<COL WIDTH="82"/>			
