<?xml version="1.0" encoding="utf-8"?>
<GRID ID="gridProductSelection" someTESTvalue="abcD"  STYLE="width:100%;height:100%;overflow:hidden;table-layout:fixed;" SCROLLX="yes" SCROLLY="yes">
	<THEAD HEIGHT="22px">
		<COLGROUP>
		<COL WIDTH="55"/>
