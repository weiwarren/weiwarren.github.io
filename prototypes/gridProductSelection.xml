		<COL WIDTH="120"/>
		<COL WIDTH="2"/>
		<COL WIDTH="200"/>
		<COL WIDTH="2"/>
		<COL WIDTH="80"/>
		<COL WIDTH="2"/>
		<COL WIDTH="80"/>
		<COL WIDTH="2"/>		
		</COLGROUP>
		<TR SORTABLE="yes" ONCLICK="return fnGridProductListCol_onClick()"  STYLE="width:97%;text-overflow: ellipsis;overflow: hidden;VERTICAL-ALIGN:middle;">
			<TD FIELD="chTowCode" SORTABLE="yes" SORTED="no" DATATYPE="text" ORDER="ascending" ALIGN="left">
				<NOBR>
					<LABEL ID="CHchTowCode" TERMCODE="chTowCode">Product Code</LABEL>
